<body>
    <script>
    let table = Array()
    for (let i = 65; i < 91; i++) {
        table.push(String.fromCharCode(i))     
    }
     console.log(table);
    for (let index = 0; index < table.length; index++) {
        c= document.createElement('button');
        c.innerHTML = table[index]
        c.addEventListener('click', function() {
        alert(table[index]);
    });
    document.body.appendChild(c); 
    }
    </script>
</body>
